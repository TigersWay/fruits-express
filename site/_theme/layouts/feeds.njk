---
eleventyExcludeFromCollections: true
permalink: "{{ section.prefix }}/feeds.xml"
---
<?xml version="1.0" encoding="UTF-8"?>
<!-- {{ pkg.name }} v{{ pkg.version }} | {{ pkg.license }} | {{ pkg.author | safe }} -->
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.title }}</title>
  <updated>{{ build.time | dateISO }}</updated>
  <id>{{ '/' | absoluteUrl }}</id>
  <link rel="self" type="application/atom+xml" href="{{ page.url | absoluteUrl }}"/>
  <author><name>{{ pkg.name }}</name></author>
{% for item in collections[section.path] | sort(attribute='date') %}
  <entry>
    <title type="html"><![CDATA[{{ item.data.title or site.title }}]]></title>
    <id>{{ item.url | absoluteUrl }}</id>
    <updated>{{ item.date | dateISO }}</updated>
    <content type="html"><![CDATA[{{ item.templateContent }}]]></content>
  </entry>
{% endfor %}
</feed>